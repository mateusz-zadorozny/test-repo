{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "create-block/accordion-module",
  "version": "0.1.0",
  "title": "[FH] Accordion Module (FAQ)",
  "category": "widgets",
  "icon": "list-view",
  "description": "Interactive (unfolding) module prepared for questions and answers section. Answers visible after clicking on chosen question.",
  "supports": {
    "html": false
  },
  "textdomain": "accordion-module",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "attributes": {
    "title": {
      "type": "string"
    },
    "content": {
      "type": "string"
    },
    "items": {
      "type": "array",
      "default": []
    }
  }
}