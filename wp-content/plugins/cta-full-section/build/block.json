{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "create-block/cta-full-section",
  "version": "0.1.0",
  "title": "[FH] CTA Full Section",
  "category": "widgets",
  "icon": "share-alt2",
  "description": "Full width interactive section with a button leading to specific section or outside link. Two seperate images available for desktop and mobile views.",
  "supports": {
    "html": false
  },
  "textdomain": "cta-full-section",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "attributes": {
    "title": {
      "type": "string"
    },
    "content": {
      "type": "string"
    },
    "contentLink": {
      "type": "string"
    },
    "contentLinkText": {
      "type": "string"
    },
    "backgroundImage": {
      "type": "string"
    },
    "mobileBackgroundImage": {
      "type": "string"
    }
  }
}